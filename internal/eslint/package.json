{
  "name": "@langchain/eslint",
  "version": "0.1.0",
  "description": "Shared ESLint configuration for LangChain.js projects",
  "author": "LangChain",
  "license": "MIT",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "type": "module",
  "keywords": [
    "eslint",
    "config",
    "langchain"
  ],
  "peerDependencies": {
    "eslint": "^9.34.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@eslint/js": "^9.34.0",
    "@types/node": "^24.3.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "typescript-eslint": "^8.40.0"
  },
  "devDependencies": {
    "eslint": "^9.34.0",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
