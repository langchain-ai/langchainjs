{
  "name": "@langchain/standard-tests",
  "version": "0.0.19",
  "description": "Standard tests for LangChain.js",
  "author": "LangChain",
  "license": "MIT",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:langchain-ai/langchainjs.git"
  },
  "homepage": "https://github.com/langchain-ai/langchainjs/tree/main/libs/langchain-standard-tests/",
  "scripts": {
    "build": "exit 0",
    "lint:eslint": "eslint --cache src/",
    "lint:dpdm": "dpdm --skip-dynamic-imports circular --exit-code circular:1 --no-warning --no-tree src/*.ts src/**/*.ts",
    "lint": "pnpm lint:eslint && pnpm lint:dpdm",
    "lint:fix": "pnpm lint:eslint --fix && pnpm lint:dpdm",
    "clean": "rm -rf .turbo",
    "format": "prettier --write \"src\"",
    "format:check": "prettier --check \"src\""
  },
  "dependencies": {
    "@jest/globals": "^29.5.0",
    "@langchain/core": "workspace:*",
    "zod": "^3.25.76 || ^4"
  },
  "devDependencies": {
    "@langchain/eslint": "workspace:*",
    "@langchain/tsconfig": "workspace:*",
    "@tsconfig/recommended": "^1.0.3",
    "dpdm": "^3.14.0",
    "eslint": "^9.34.0",
    "prettier": "^3.5.0",
    "typescript": "~5.8.3"
  },
  "peerDependencies": {
    "vitest": "^3.2.4"
  },
  "peerDependenciesMeta": {
    "vitest": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./vitest": {
      "types": "./src/vitest.ts",
      "import": "./src/vitest.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src/",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ]
}
