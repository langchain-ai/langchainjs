{
  "$schema": "https://turbo.build/schema.json",
  "remoteCache": {
    "enabled": true
  },
  "globalDependencies": ["**/.env"],
  "pipeline": {
    "@langchain/scripts#build": {
      "outputs": [
        "./libs/langchain-scripts/dist/**",
        "*.js",
        "*.cjs",
        "*.d.ts"
      ],
      "inputs": [
        "./libs/langchain-scripts/src/",
        "./libs/langchain-scripts/scripts/**",
        "./libs/langchain-scripts/package.json",
        "./libs/langchain-scripts/tsconfig.json"
      ]
    },
    "@langchain/core#build": {
      "dependsOn": [],
      "outputs": ["dist/**", "dist-cjs/**", "*.js", "*.cjs", "*.d.ts"],
      "inputs": ["src/**", "scripts/**", "package.json", "tsconfig.json"]
    },
    "@langchain/anthropic#build": {
      "dependsOn": [],
      "outputs": ["dist/**", "dist-cjs/**", "*.js", "*.cjs", "*.d.ts"],
      "inputs": ["src/**", "scripts/**", "package.json", "tsconfig.json"]
    },
    "@langchain/openai#build": {
      "dependsOn": [],
      "outputs": ["dist/**", "dist-cjs/**", "*.js", "*.cjs", "*.d.ts"],
      "inputs": ["src/**", "scripts/**", "package.json", "tsconfig.json"]
    },
    "@langchain/community#build": {
      "dependsOn": [],
      "outputs": ["dist/**", "dist-cjs/**", "*.js", "*.cjs", "*.d.ts"],
      "inputs": ["src/**", "scripts/**", "package.json", "tsconfig.json"]
    },
    "build": {
      "dependsOn": [
        "@langchain/scripts#build",
        "@langchain/core#build",
        "^build"
      ],
      "outputs": ["dist/**", "dist-cjs/**", "*.js", "*.cjs", "*.d.ts"],
      "inputs": ["src/**", "scripts/**", "package.json", "tsconfig.json"]
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": [
        "langchain/src/**/*.ts",
        "langchain-core/src/**/*.ts",
        "libs/**/src/**/*.ts",
        "docs/core_docs/**/*.js",
        "examples/src/**/*.ts",
        "**/*.eslintcache"
      ],
      "inputs": [
        "langchain/src/**/*.ts",
        "langchain-core/src/**/*.ts",
        "libs/**/src/**/*.ts",
        "docs/core_docs/**/*.js",
        "examples/src/**/*.ts"
      ]
    },
    "format": {
      "outputs": []
    },
    "format:check": {
      "outputs": []
    },
    "test": {
      "outputs": [],
      "dependsOn": ["^build"]
    },
    "test:ci": {
      "outputs": [],
      "dependsOn": ["test"]
    },
    "test:integration": {
      "outputs": [],
      "dependsOn": ["^build"]
    },
    "precommit": {},
    "start": {
      "cache": false
    },
    "build:vercel": {
      "dependsOn": ["^build:vercel"]
    }
  }
}
