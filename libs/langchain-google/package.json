{
  "name": "@langchain/google",
  "version": "0.2.12",
  "description": "Google auth based authentication support for Google services including VertexAI",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "main": "./node.js",
  "types": "./node.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:langchain-ai/langchainjs.git"
  },
  "homepage": "https://github.com/langchain-ai/langchainjs/tree/main/libs/langchain-google-gauth/",
  "scripts": {
    "build": "yarn turbo:command build:internal --filter=@langchain/google-gauth",
    "build:internal": "yarn lc_build --create-entrypoints --pre --tree-shaking",
    "lint:eslint": "NODE_OPTIONS=--max-old-space-size=4096 eslint --cache --ext .ts,.js src/",
    "lint:dpdm": "dpdm --skip-dynamic-imports circular --exit-code circular:1 --no-warning --no-tree src/*.ts src/**/*.ts",
    "lint": "yarn lint:eslint && yarn lint:dpdm",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:dpdm",
    "clean": "rm -rf .turbo dist/",
    "prepack": "yarn build",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --testPathIgnorePatterns=\\.int\\.test.ts --testTimeout 30000 --maxWorkers=50%",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch --testPathIgnorePatterns=\\.int\\.test.ts",
    "test:single": "NODE_OPTIONS=--experimental-vm-modules yarn run jest --config jest.config.cjs --testTimeout 100000",
    "test:int": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern=\\.int\\.test.ts --testTimeout 100000 --maxWorkers=50%",
    "test:standard:unit": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern=\\.standard\\.test.ts --testTimeout 100000 --maxWorkers=50%",
    "test:standard:int": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern=\\.standard\\.int\\.test.ts --testTimeout 100000 --maxWorkers=50%",
    "test:standard": "yarn test:standard:unit && yarn test:standard:int",
    "test:e2e": "yarn workspace @langchain/google-e2e test",
    "format": "prettier --config .prettierrc --write \"src\"",
    "format:check": "prettier --config .prettierrc --check \"src\""
  },
  "author": "LangChain",
  "license": "MIT",
  "dependencies": {
    "@langchain/google-common": "^0.2.10",
    "google-auth-library": "^10.1.0",
    "web-auth-library": "^1.0.3"
  },
  "peerDependencies": {
    "@langchain/core": ">=0.3.58 <0.4.0"
  },
  "devDependencies": {
    "@langchain/core": "workspace:*",
    "@langchain/scripts": ">=0.1.0 <0.2.0",
    "@swc/core": "^1.3.90",
    "@swc/jest": "^0.2.29",
    "@tsconfig/recommended": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "dotenv": "^16.3.1",
    "dpdm": "^3.14.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "release-it": "^18.1.2",
    "typescript": "~5.8.3",
    "vitest": "^3.2.4",
    "zod": "^3.25.67"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": {
        "import": "./index.d.ts",
        "require": "./index.d.cts",
        "default": "./index.d.ts"
      },
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./utils": {
      "types": {
        "import": "./utils.d.ts",
        "require": "./utils.d.cts",
        "default": "./utils.d.ts"
      },
      "import": "./utils.js",
      "require": "./utils.cjs"
    },
    "./types": {
      "types": {
        "import": "./types.d.ts",
        "require": "./types.d.cts",
        "default": "./types.d.ts"
      },
      "import": "./types.js",
      "require": "./types.cjs"
    },
    "./web": {
      "types": {
        "import": "./web.d.ts",
        "require": "./web.d.cts",
        "default": "./web.d.ts"
      },
      "import": "./web.js",
      "require": "./web.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "index.cjs",
    "index.js",
    "index.d.ts",
    "index.d.cts",
    "utils.cjs",
    "utils.js",
    "utils.d.ts",
    "utils.d.cts",
    "types.cjs",
    "types.js",
    "types.d.ts",
    "types.d.cts",
    "web.cjs",
    "web.js",
    "web.d.ts",
    "web.d.cts"
  ]
}
