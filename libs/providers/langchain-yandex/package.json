{
  "name": "@langchain/yandex",
  "version": "1.0.0",
  "description": "Yandex integration for LangChain.js",
  "author": "LangChain",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:langchain-ai/langchainjs.git"
  },
  "homepage": "https://github.com/langchain-ai/langchainjs/tree/main/libs/langchain-yandex/",
  "scripts": {
    "build": "turbo build:compile --filter @langchain/yandex",
    "build:compile": "tsdown",
    "lint:eslint": "eslint --cache src/",
    "lint:dpdm": "dpdm --skip-dynamic-imports circular --exit-code circular:1 --no-warning --no-tree src/*.ts src/**/*.ts",
    "lint": "pnpm lint:eslint && pnpm lint:dpdm",
    "lint:fix": "pnpm lint:eslint --fix && pnpm lint:dpdm",
    "clean": "rm -rf .turbo dist/",
    "test:watch": "vitest --watch",
    "test:int": "vitest --mode int",
    "format": "prettier --config .prettierrc --write \"src\"",
    "format:check": "prettier --config .prettierrc --check \"src\""
  },
  "peerDependencies": {
    "@langchain/core": "^1.0.0"
  },
  "devDependencies": {
    "@langchain/core": "workspace:*",
    "@langchain/eslint": "workspace:*",
    "@tsconfig/recommended": "^1.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "dotenv": "^16.3.1",
    "dpdm": "^3.14.0",
    "eslint": "^9.34.0",
    "prettier": "^2.8.3",
    "rollup": "^4.5.2",
    "vitest": "^3.2.4",
    "typescript": "~5.8.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./chat_models": {
      "import": "./dist/chat_models.js",
      "require": "./dist/chat_models.cjs"
    },
    "./embeddings": {
      "import": "./dist/embeddings.js",
      "require": "./dist/embeddings.cjs"
    },
    "./llms": {
      "import": "./dist/llms.js",
      "require": "./dist/llms.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "CHANGELOG.md",
    "README.md",
    "LICENSE"
  ],
  "module": "./dist/index.js"
}