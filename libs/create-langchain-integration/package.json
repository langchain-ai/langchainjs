{
  "name": "create-langchain-integration",
  "version": "0.0.12",
  "repository": {
    "type": "git",
    "url": "https://github.com/langchain-ai/langchainjs",
    "directory": "libs/create-langchain-integration"
  },
  "bin": "./dist/index.js",
  "scripts": {
    "dev": "ncc build ./index.ts -w -o dist/",
    "build": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register && cp ./template/.env.example ./template/.eslintrc.cjs ./template/.prettierrc ./template/.release-it.json ./dist/template",
    "format": "prettier --write \"./helpers\"",
    "format:check": "prettier --check \"./helpers\"",
    "lint:eslint": "eslint --cache  ./helpers",
    "lint": "eslint --cache  ./helpers",
    "lint:fix": "eslint --cache --fix --ext .ts,.js ./helpers"
  },
  "devDependencies": {
    "@langchain/eslint": "workspace:*",
    "@langchain/tsconfig": "workspace:*",
    "@types/prompts": "^2",
    "@types/validate-npm-package-name": "4.0.2",
    "@vercel/ncc": "^0.38.4",
    "commander": "^2.20.0",
    "conf": "^15.1.0",
    "dotenv": "^16.3.1",
    "eslint": "^9.34.0",
    "fast-glob": "^3.3.2",
    "picocolors": "^1.0.0",
    "prettier": "^3.5.0",
    "prompts": "^2.4.2",
    "typescript": "~5.8.3",
    "update-check": "^1.5.4",
    "validate-npm-package-name": "^7.0.2"
  }
}
