{
  "name": "@langchain/agents",
  "version": "0.3.66",
  "description": "LangChain.js Agents",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:langchain-ai/langchainjs.git"
  },
  "homepage": "https://github.com/langchain-ai/langchainjs/tree/main/langchain-core/",
  "scripts": {
    "build": "pnpm --filter @langchain/build compile @langchain/agents",
    "clean": "rm -rf .turbo dist/",
    "lint:eslint": "NODE_OPTIONS=--max-old-space-size=4096 eslint --cache --ext .ts,.js src/",
    "lint:dpdm": "dpdm --skip-dynamic-imports circular --exit-code circular:1 --no-warning --transform --no-tree src/*.ts src/**/*.ts",
    "lint": "pnpm lint:eslint && pnpm lint:dpdm",
    "lint:fix": "pnpm lint:eslint --fix && pnpm lint:dpdm",
    "release": "release-it --only-version --config .release-it.json",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:integration": "vitest --run --mode int",
    "format": "prettier --config .prettierrc --write \"src\"",
    "format:check": "prettier --config .prettierrc --check \"src\""
  },
  "author": "LangChain",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "workspace:*",
    "@langchain/langgraph": "^0.4.5",
    "@langchain/langgraph-checkpoint": "^0.1.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "@langchain/anthropic": "workspace:*",
    "dpdm": "^3.14.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "vitest": "^3.2.4",
    "zod": "^3.25.32"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "llm",
    "ai",
    "agents",
    "agentic",
    "chain",
    "prompt",
    "prompt engineering",
    "chatgpt",
    "machine learning",
    "ml",
    "openai",
    "embeddings",
    "vectorstores"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "input": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/"
  ]
}
